{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "title": "TeCore Odoo 14.0.2",
      "description": "Open-source business apps version 14.0.2 tecorp for the Bolivian localization",
      "categories": [
        "project-management"
      ],
      "platform": "linux",
      "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/odoo.png",
      "image": "tecorpapps/tcp_odoo:14.0.2",
      "network": "web",
      "restart_policy": "always",
      "administrator-only": true,
      "interactive": true,
      "note": "",
      "env": [
        {
          "name": "PORTAINER_ENVIRONMENT",
          "label": "Environment",
          "select": [
            {
              "text": "Testing",
              "value": "tst",
              "default": true
            },
            {
              "text": "Production",
              "value": "prd"
            }
          ]
        },
        {
          "name": "WEB_CONTAINER_NAME",
          "label": "Instance Name",
          "default": "tecore_od14base"
        },
        {
          "name": "WEB_IMAGE_NAME",
          "label": "Image Name",
          "default": "tecorpapps/tcp_odoo"
        },
        {
          "name": "WEB_IMAGE_TAG",
          "label": "Image Tag",
          "default": "14.0.2"
        },
        {
          "name": "WEB_PORT",
          "label": "Port",
          "default": "9061"
        },
        {
          "name": "WEB_LONGPOLLING_PORT",
          "label": "LongPollingPort",
          "default": "9062"
        },
        {
          "name": "WEB_IMAGE_NAME",
          "label": "Image Name",
          "default": "tecorpapps/tcp_odoo"
        },
        {
          "name": "DB_HOST",
          "label": "PG Host",
          "default": "db_tecore_od14base"
        },
        {
          "name": "DB_IMAGE",
          "label": "PG Image",
          "default": "postgres"
        },
        {
          "name": "DB_TAG",
          "label": "PG Version",
          "default": "14"
        },
        {
          "name": "DB_PORT",
          "label": "PG Port",
          "default": "5434"
        }
        ,
        {
          "name": "DB_USER",
          "label": "PG User",
          "default": "dbodoo"
        },
        {
          "name": "DB_PASSWD",
          "label": "PG Password",
          "default": "dbodoo"
        },
        {
          "name": "DB_NAME",
          "label": "PG DB Name",
          "default": "postgres",
          "preset": true
        }
      ],
      "ports": [
        "${WEB_PORT}:8069/tcp",
        "${WEB_LONGPOLLING_PORT}:8072/tcp"
      ],
      "volumes": [
        {
          "container": "/var/lib/odoo"
        },
        {
          "container": "/mnt/extra-addons"
        }
      ]
    },
